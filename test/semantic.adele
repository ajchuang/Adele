####### Math
### int

    int add(int a, int b)
      return a+b;
    end

    int sub(int a, int b)
      return a-b;
    end

    int mul(int a, int b)
      return a*b;
    end

    int div(int a, int b)
      return a/b;
    end

### float

    float fadd(float a, float b)
      return a+b;
    end

    float fsub(float a, float b)
      return a-b;
    end

    float fmul(float a, float b)
      return a*b;
    end

    float fdiv(float a, float b)
      return a/b;
    end

####### Special Character
    string newline()
        return "\n";
    end

    string backslash()
        return "\\";
    end

    string hor_tab()
        return "\t";
    end

    string single_quote()
        return "\'";
    end

    string backspace()
        return "\b";
    end

    string double_quote()
        return "\"";
    end

####### Flow Control
    bool if_pos_int()
        int a = 4;
        if(a + 5)
            return true;
        end
    end

    bool if_neg_int()
        int a = -4;
        if(a + 1)
            return false;
        end
    end

    int while_til_0()
        int a = 5;
        while(a > 0)
            a = a - 1;
        end
        return a;
    end

####### Array
    int array_access(int num)
        int a[5];
        a[0] = num;
        return a[0];
    end
