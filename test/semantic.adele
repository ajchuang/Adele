####### Math
### int

    int add(int a, int b)
      return a+b;
    end

    int sub(int a, int b)
      return a-b;
    end

    int mul(int a, int b)
      return a*b;
    end

    int div(int a, int b)
      return a/b;
    end

### float

    float fadd(float a, float b)
      return a+b;
    end

    float fsub(float a, float b)
      return a-b;
    end

    float fmul(float a, float b)
      return a*b;
    end

    float fdiv(float a, float b)
      return a/b;
    end

####### Special Character
    string newline()
        return "\n";
    end

    string backslash()
        return "\\";
    end

    string hor_tab()
        return "\t";
    end

    string single_quote()
        return "\'";
    end

    string backspace()
        return "\b";
    end

    string double_quote()
        return "\"";
    end

